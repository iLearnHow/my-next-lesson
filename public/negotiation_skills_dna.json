  "core_lesson_structure": {
    "question_1": {
      "concept_focus": "collaborative_vs_adversarial_mindset",
      "universal_principle": "understanding_others_works_better_than_convincing_others",
      "cognitive_target": "mindset_shift_from_winning_to_collaborating",
      "choice_architecture": {
        "option_a": "defend_position_and_convince_approach",
        "option_b": "understand_concerns_and_collaborate_approach"
      },
      "teaching_moments": {
        "option_a_response": "explain_why_positions_and_arguments_create_resistance_not_solutions",
        "option_b_response": "celebrate_collaborative_mindset_and_strategic_thinking"
      }
    },
    "question_2": {
      "concept_focus": "strategic_questioning_over_immediate_objections",
      "universal_principle": "questions_gather_information_for_better_solutions_than_arguments",
      "cognitive_target": "tactical_shift_from_arguing_to_inquiring",
      "choice_architecture": {
        "option_a": "immediately_explain_why_idea_wont_work",
        "option_b": "ask_questions_to_understand_reasoning_first"
      },
      "teaching_moments": {
        "option_a_response": "explain_how_immediate_objections_shut_down_dialogue_and_miss_opportunities",
        "option_b_response": "celebrate_strategic_questioning_and_information_gathering_approach"
      }
    },
    "question_3": {
      "concept_focus": "separating_people_from_problems",
      "universal_principle": "preserve_relationships_while_addressing_issues_directly",
      "cognitive_target": "emotional_intelligence_shift_to_dual_awareness",
      "choice_architecture": {
        "option_a": "focus_on_being_right_about_the_issue",
        "option_b": "protect_relationship_while_solving_problem_together"
      },
      "teaching_moments": {
        "option_a_response": "explain_how_being_right_can_damage_relationships_and_miss_collaborative_solutions",
        "option_b_response": "celebrate_relationship_wisdom_and_long_term_thinking"
      }
    }
  },

  "example_selector_data": {
    "question_1_examples": {
      "early_childhood": {
        "scenario": "when_you_and_friend_both_want_same_thing",
        "option_a": "try_to_get_it_first_and_convince_them_you_should_have_it",
