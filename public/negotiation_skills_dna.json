      },
      "midlife": {
        "scenario": "community_member_proposes_solution_you_have_concerns_about",
        "option_a": "outline_potential_problems_and_suggest_alternative_approaches",
        "option_b": "inquire_about_their_assumptions_and_expected_outcomes_first"
      },
      "wisdom_years": {
        "scenario": "someone_you_care_about_suggests_approach_you_worry_might_not_work",
        "option_a": "share_experience_about_why_similar_approaches_have_been_challenging",
        "option_b": "ask_gentle_questions_to_understand_their_perspective_fully"
      }
    },
    "question_3_examples": {
      "early_childhood": {
        "scenario": "disagreement_with_someone_you_care_about",
