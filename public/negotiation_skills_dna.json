        "option_b": "help_everyone_share_their_deepest_concerns_and_hopes"
      }
    },
    "question_2_examples": {
      "early_childhood": {
        "scenario": "friend_suggests_activity_you_dont_think_sounds_fun",
        "option_a": "say_that_sounds_boring_and_suggest_something_different",
        "option_b": "ask_what_they_like_about_that_activity"
      },
      "youth": {
        "scenario": "someone_proposes_idea_for_group_that_seems_problematic",
        "option_a": "immediately_point_out_potential_problems_with_their_idea",
        "option_b": "ask_questions_to_understand_how_they_think_it_would_work"
      },
      "young_adult": {
        "scenario": "someone_suggests_approach_to_shared_situation_you_think_might_not_work",
        "option_a": "explain_concerns_and_potential_problems_you_see",
